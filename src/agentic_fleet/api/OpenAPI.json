  "components": {
    "schemas": {
      "ApprovalDecision": {
        "properties": {
          "decision": {
            "type": "string",
            "title": "Decision"
          },
          "reason": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Reason"
          }
        },
        "type": "object",
        "required": ["decision"],
        "title": "ApprovalDecision"
      },
      "ChatRequest": {
        "properties": {
          "conversation_id": {
            "type": "string",
            "title": "Conversation Id",
            "description": "Conversation identifier"
          },
          "message": {
            "type": "string",
            "title": "Message",
            "description": "User message content"
          },
          "stream": {
            "type": "boolean",
            "title": "Stream",
            "description": "Whether to stream the response as SSE",
            "default": false
          }
        },
        "type": "object",
        "required": ["conversation_id", "message"],
        "title": "ChatRequest"
      },
      "ConversationResponse": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id",
            "description": "Conversation identifier"
          },
          "title": {
            "type": "string",
            "title": "Title",
            "description": "Conversation title"
          },
          "created_at": {
            "type": "integer",
            "title": "Created At",
            "description": "Creation timestamp"
          },
          "messages": {
            "items": {
              "$ref": "#/components/schemas/MessageResponse"
            },
            "type": "array",
            "title": "Messages",
            "description": "Ordered list of messages"
          }
        },
        "additionalProperties": true,
        "type": "object",
        "required": ["id", "title", "created_at", "messages"],
        "title": "ConversationResponse"
      },
      "ConversationsListResponse": {
        "properties": {
          "items": {
            "items": {
              "$ref": "#/components/schemas/ConversationResponse"
            },
            "type": "array",
            "title": "Items",
            "description": "List of conversations"
          }
        },
        "additionalProperties": true,
        "type": "object",
        "required": ["items"],
        "title": "ConversationsListResponse"
      },
      "CreateWorkflowRequest": {
        "properties": {
          "task": {
            "type": "string",
            "title": "Task",
            "description": "Task description for the workflow"
          },
          "config": {
            "anyOf": [
              {
                "additionalProperties": {
                  "type": "integer"
                },
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "title": "Config",
            "description": "Optional configuration overrides"
          }
        },
        "type": "object",
        "required": ["task"],
        "title": "CreateWorkflowRequest",
        "description": "Request to create a new workflow."
      },
      "CreateWorkflowResponse": {
        "properties": {
          "workflow_id": {
            "type": "string",
            "title": "Workflow Id",
            "description": "Unique workflow identifier"
          },
          "task": {
            "type": "string",
            "title": "Task",
            "description": "Task description"
          },
          "status": {
            "type": "string",
            "title": "Status",
            "description": "Workflow status"
          }
        },
        "type": "object",
        "required": ["workflow_id", "task", "status"],
        "title": "CreateWorkflowResponse",
        "description": "Response for workflow creation."
      },
      "EntityDetailResponse": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id",
            "description": "Entity identifier (workflow ID)"
          },
          "name": {
            "type": "string",
            "title": "Name",
            "description": "Entity display name"
          },
          "description": {
            "type": "string",
            "title": "Description",
            "description": "Entity description",
            "default": ""
          },
          "input_schema": {
            "$ref": "#/components/schemas/InputSchema",
            "description": "Input schema for the entity"
          }
        },
        "additionalProperties": true,
        "type": "object",
        "required": ["id", "name", "input_schema"],
        "title": "EntityDetailResponse",
        "description": "Entity information matching OpenAI Responses API format."
      },
      "EntityListResponse": {
        "properties": {
          "entities": {
            "items": {
              "$ref": "#/components/schemas/EntityDetailResponse"
            },
            "type": "array",
            "title": "Entities",
            "description": "List of available entities"
          }
        },
        "additionalProperties": true,
        "type": "object",
        "required": ["entities"],
        "title": "EntityListResponse",
        "description": "Response containing list of available entities."
      },
      "EntityReloadResponse": {
        "properties": {
